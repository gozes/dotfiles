{
    "languageserver": {
        "nim": {
            "command": "nimlsp",
            "filetypes": [
                "nim"
            ]
        }
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "javascript",
        "graphql",
        "html",
        "python"
    ],
    "python.jediEnabled": false,
    "yaml.schemas": {
        "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": "/*"
    },
    "yaml.schemaStore.enable": true,
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap sequence",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Join sequence"
    ]
}
